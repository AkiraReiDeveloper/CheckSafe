<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/selectcolorswitch">

    <TextView
        android:id="@+id/textGrupoU"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="@string/txtGrupo"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textSexoU"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toEndOf="@+id/textGrupoL" />

    <TextView
        android:id="@+id/textSexoU"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="@string/textSexoU"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/separador"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toEndOf="@+id/textSexoL" />

    <TextView
        android:id="@+id/textGrupoL"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/txtGrupo"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textSexoL"
        app:layout_constraintStart_toEndOf="@+id/imgConsultarListaUsuarios" />

    <TextView
        android:id="@+id/textSexoL"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/textSexoL"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/separador"
        app:layout_constraintStart_toEndOf="@+id/imgConsultarListaUsuarios"
        tools:text="Sexo" />

    <TextView
        android:id="@+id/textEmailU"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="@string/txtEmail"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textGradoU"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toEndOf="@+id/textEmailL" />

    <TextView
        android:id="@+id/textGradoU"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="@string/txtGrado"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textGrupoU"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toEndOf="@+id/textGradoL" />

    <TextView
        android:id="@+id/textApellidosU"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="@string/txtApellidos"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textEmailU"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textApellidosL" />

    <TextView
        android:id="@+id/textNoCuentaU"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="@string/txtNoCuenta"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textNombreU"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textNoCuentaL"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textNombreU"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:hint="@string/txtNombres"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textApellidosU"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textNombreL" />

    <TextView
        android:id="@+id/textNoCuentaL"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/txtNoCuenta"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textNombreL"
        app:layout_constraintStart_toEndOf="@+id/imgConsultarListaUsuarios"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imgConsultarListaUsuarios"
        android:layout_width="79dp"
        android:layout_height="79dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="8dp"
        android:contentDescription="@string/imgConsultarUsuarios"
        app:layout_constraintBottom_toBottomOf="@+id/separador"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_base" />

    <TextView
        android:id="@+id/textNombreL"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/txtNombres"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textApellidosL"
        app:layout_constraintStart_toEndOf="@+id/imgConsultarListaUsuarios" />

    <TextView
        android:id="@+id/textApellidosL"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/txtApellidos"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textEmailL"
        app:layout_constraintStart_toEndOf="@+id/imgConsultarListaUsuarios" />

    <TextView
        android:id="@+id/textEmailL"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/txtEmail"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textGradoL"
        app:layout_constraintStart_toEndOf="@+id/imgConsultarListaUsuarios" />

    <TextView
        android:id="@+id/textGradoL"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/txtGrado"
        android:textColor="@color/colorAccent"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textGrupoL"
        app:layout_constraintStart_toEndOf="@+id/imgConsultarListaUsuarios" />

    <TextView
        android:id="@+id/separador"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/separador"
        android:textAllCaps="false"
        android:textColor="@color/colorPrimary"
        android:textSize="14sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />
</android.support.constraint.ConstraintLayout>